<%# dark red tall rectangle %>
<div
class="rectangle-left"
style="
background-color: #950101;
width: 60px;
height: 280px;
position: absolute;
z-index: 99;
left: 0;
border-bottom-right-radius: 0.7rem;
">
  <%# text inside rectangle %>
  <h5
    class="mt-3 ms-2"
    style="
      writing-mode: vertical-rl;
      color: #F0DFDF;
      font-size: 15px;
    "
  >
    <%= title || "default text" %>
  </h5>
</div>
